<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.corso.hibernateEjpa">
    <class name="Archivio" table="archivio">
        <id name="id" type="java.lang.Integer" column="id_archivio">
            <generator class="increment"/>
        </id>
        <property name="numberCd"  column="numberCd"/>
        <property name="idBrano"  column="idBrano"/>
    </class>

</hibernate-mapping>
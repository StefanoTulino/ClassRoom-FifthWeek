<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.corso.hibernateEjpa">
    <class name="Cd" table="cd">
        <id name="id" type="java.lang.Integer" column="id_cd">
            <generator class="increment"/>
        </id>
        <property name="name" type="java.lang.Integer"  column="name"/>
        <property name="numberCd" type="java.lang.Integer"  column="numberCd"/>
        <property name="idBrano" type="java.lang.Integer"  column="idBrano"/>
        <property name="numBrani" type="java.lang.Integer"  column="numBrani"/>

    </class>

</hibernate-mapping>